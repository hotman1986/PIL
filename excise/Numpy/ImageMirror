from PIL import Image
from numpy import* 
from pylab import*
im0= array(Image.open('D://thesis/haze/1.jpg'))
im = array(Image.open('D://thesis/haze/1.jpg').convert('L'))

im2 = 255 - im #反相处理
im3 = (100.0/255)*im + 100 #把图像的像素变换到100-200的区间
im4 = 255.0 * (im/255.0) ** 2

figure()
imshow(im0)
show()

